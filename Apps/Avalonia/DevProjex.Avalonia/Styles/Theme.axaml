<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
  <ResourceDictionary x:Key="Light">
    <!-- Base Light theme (no effects) - clean neutral white -->
    <Color x:Key="AppBackgroundColor">#FFFFFF</Color>
    <Color x:Key="AppPanelColor">#F3F3F3</Color>
    <Color x:Key="AppBorderColor">#C7C7C7</Color>
    <Color x:Key="AppAccentColor">#0078D4</Color>
    <Color x:Key="AppAccentHoverColor">#006EBF</Color>
    <Color x:Key="AppAccentPressedColor">#005CA0</Color>
    <Color x:Key="AppTextColor">#1A1A1A</Color>
    <Color x:Key="AppMutedTextColor">#5E5E5E</Color>
    <Color x:Key="TreeSelectionColor">#DCEEFF</Color>
    <Color x:Key="TreeSearchHighlightColor">#F7C76B</Color>
    <Color x:Key="TreeSearchCurrentColor">#F9A825</Color>
    <Color x:Key="MenuHoverColor">#E6E6E6</Color>
    <Color x:Key="MenuPressedColor">#D9D9D9</Color>
    <Color x:Key="ToastSurfaceColor">#CCF3F3F3</Color>

    <!-- Drop Zone Colors - Light Theme -->
    <Color x:Key="DropZoneBorderColor">#0066CC</Color>
    <Color x:Key="DropZoneInnerColor">#70F0F0F0</Color>
    <Color x:Key="DropZoneFolderBackColor">#0078D4</Color>
    <Color x:Key="DropZoneFolderFrontColor">#3DA0E3</Color>
    <Color x:Key="DropZoneArrowColor">#FFFFFF</Color>
    <Color x:Key="DropZoneKbdColor">#20000000</Color>
    <Color x:Key="SegmentSelectedHoverOverlayColor">#1A000000</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="AppBackgroundBrush" Color="{StaticResource AppBackgroundColor}" />
    <SolidColorBrush x:Key="AppPanelBrush" Color="{StaticResource AppPanelColor}" />
    <SolidColorBrush x:Key="AppBorderBrush" Color="{StaticResource AppBorderColor}" />
    <SolidColorBrush x:Key="AppAccentBrush" Color="{StaticResource AppAccentColor}" />
    <SolidColorBrush x:Key="AppAccentHoverBrush" Color="{StaticResource AppAccentHoverColor}" />
    <SolidColorBrush x:Key="AppAccentPressedBrush" Color="{StaticResource AppAccentPressedColor}" />
    <SolidColorBrush x:Key="PrimaryActionForegroundBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="AppTextBrush" Color="{StaticResource AppTextColor}" />
    <SolidColorBrush x:Key="AppMutedTextBrush" Color="{StaticResource AppMutedTextColor}" />
    <SolidColorBrush x:Key="TreeSelectionBrush" Color="{StaticResource TreeSelectionColor}" />
    <SolidColorBrush x:Key="TreeSelectionTextBrush" Color="{StaticResource AppTextColor}" />
    <SolidColorBrush x:Key="TreeSearchHighlightBrush" Color="{StaticResource TreeSearchHighlightColor}" />
    <SolidColorBrush x:Key="TreeSearchCurrentBrush" Color="{StaticResource TreeSearchCurrentColor}" />
    <SolidColorBrush x:Key="TreeSearchHighlightTextBrush" Color="#000000" />
    <SolidColorBrush x:Key="MenuPopupBrush" Color="{StaticResource AppPanelColor}"/>
    <SolidColorBrush x:Key="MenuChildPopupBrush" Color="{StaticResource AppPanelColor}"/>
    <SolidColorBrush x:Key="MenuHoverBrush" Color="{StaticResource MenuHoverColor}"/>
    <SolidColorBrush x:Key="MenuPressedBrush" Color="{StaticResource MenuPressedColor}"/>
    <SolidColorBrush x:Key="MenuChildHoverBrush" Color="{StaticResource MenuHoverColor}"/>
    <SolidColorBrush x:Key="MenuChildPressedBrush" Color="{StaticResource MenuPressedColor}"/>
    <SolidColorBrush x:Key="ToastSurfaceBrush" Color="{StaticResource ToastSurfaceColor}" />

    <!-- Drop Zone Brushes - Light Theme -->
    <SolidColorBrush x:Key="DropZoneBorderBrush" Color="{StaticResource DropZoneBorderColor}" />
    <SolidColorBrush x:Key="DropZoneInnerBrush" Color="{StaticResource DropZoneInnerColor}" />
    <SolidColorBrush x:Key="DropZoneFolderBackBrush" Color="{StaticResource DropZoneFolderBackColor}" />
    <SolidColorBrush x:Key="DropZoneFolderFrontBrush" Color="{StaticResource DropZoneFolderFrontColor}" />
    <SolidColorBrush x:Key="DropZoneArrowBrush" Color="{StaticResource DropZoneArrowColor}" />
    <SolidColorBrush x:Key="DropZoneKbdBrush" Color="{StaticResource DropZoneKbdColor}" />
    <SolidColorBrush x:Key="SegmentSelectedHoverOverlayBrush" Color="{StaticResource SegmentSelectedHoverOverlayColor}" />
  </ResourceDictionary>

  <ResourceDictionary x:Key="Dark">
    <!-- Base Dark theme (no effects) - deep dark, not gray -->
    <Color x:Key="AppBackgroundColor">#121214</Color>
    <Color x:Key="AppPanelColor">#17171A</Color>
    <Color x:Key="AppBorderColor">#2B2F36</Color>
    <Color x:Key="AppAccentColor">#2D8CFF</Color>
    <Color x:Key="AppAccentHoverColor">#257BDD</Color>
    <Color x:Key="AppAccentPressedColor">#1F6BC3</Color>
    <Color x:Key="AppTextColor">#E7E9EF</Color>
    <Color x:Key="AppMutedTextColor">#9AA3B2</Color>
    <Color x:Key="TreeSelectionColor">#254861</Color>
    <Color x:Key="TreeSearchHighlightColor">#E6A23A</Color>
    <Color x:Key="TreeSearchCurrentColor">#F39C12</Color>
    <Color x:Key="MenuHoverColor">#3A3D44</Color>
    <Color x:Key="MenuPressedColor">#4A4D54</Color>
    <Color x:Key="ToastSurfaceColor">#B31B1D22</Color>

    <!-- Drop Zone Colors - Dark Theme -->
    <Color x:Key="DropZoneBorderColor">#4DA6FF</Color>
    <Color x:Key="DropZoneInnerColor">#50181820</Color>
    <Color x:Key="DropZoneFolderBackColor">#1E6BBF</Color>
    <Color x:Key="DropZoneFolderFrontColor">#2D8CFF</Color>
    <Color x:Key="DropZoneArrowColor">#FFFFFF</Color>
    <Color x:Key="DropZoneKbdColor">#25FFFFFF</Color>
    <Color x:Key="SegmentSelectedHoverOverlayColor">#22000000</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="AppBackgroundBrush" Color="{StaticResource AppBackgroundColor}" />
    <SolidColorBrush x:Key="AppPanelBrush" Color="{StaticResource AppPanelColor}" />
    <SolidColorBrush x:Key="AppBorderBrush" Color="{StaticResource AppBorderColor}" />
    <SolidColorBrush x:Key="AppAccentBrush" Color="{StaticResource AppAccentColor}" />
    <SolidColorBrush x:Key="AppAccentHoverBrush" Color="{StaticResource AppAccentHoverColor}" />
    <SolidColorBrush x:Key="AppAccentPressedBrush" Color="{StaticResource AppAccentPressedColor}" />
    <SolidColorBrush x:Key="PrimaryActionForegroundBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="AppTextBrush" Color="{StaticResource AppTextColor}" />
    <SolidColorBrush x:Key="AppMutedTextBrush" Color="{StaticResource AppMutedTextColor}" />
    <SolidColorBrush x:Key="TreeSelectionBrush" Color="{StaticResource TreeSelectionColor}" />
    <SolidColorBrush x:Key="TreeSelectionTextBrush" Color="{StaticResource AppTextColor}" />
    <SolidColorBrush x:Key="TreeSearchHighlightBrush" Color="{StaticResource TreeSearchHighlightColor}" />
    <SolidColorBrush x:Key="TreeSearchCurrentBrush" Color="{StaticResource TreeSearchCurrentColor}" />
    <SolidColorBrush x:Key="TreeSearchHighlightTextBrush" Color="#000000" />
    <SolidColorBrush x:Key="MenuPopupBrush" Color="{StaticResource AppPanelColor}" />
    <SolidColorBrush x:Key="MenuChildPopupBrush" Color="{StaticResource AppPanelColor}"/>
    <SolidColorBrush x:Key="MenuHoverBrush" Color="{StaticResource MenuHoverColor}"/>
    <SolidColorBrush x:Key="MenuPressedBrush" Color="{StaticResource MenuPressedColor}"/>
    <SolidColorBrush x:Key="MenuChildHoverBrush" Color="{StaticResource MenuHoverColor}"/>
    <SolidColorBrush x:Key="MenuChildPressedBrush" Color="{StaticResource MenuPressedColor}"/>
    <SolidColorBrush x:Key="ToastSurfaceBrush" Color="{StaticResource ToastSurfaceColor}" />

    <!-- Drop Zone Brushes - Dark Theme -->
    <SolidColorBrush x:Key="DropZoneBorderBrush" Color="{StaticResource DropZoneBorderColor}" />
    <SolidColorBrush x:Key="DropZoneInnerBrush" Color="{StaticResource DropZoneInnerColor}" />
    <SolidColorBrush x:Key="DropZoneFolderBackBrush" Color="{StaticResource DropZoneFolderBackColor}" />
    <SolidColorBrush x:Key="DropZoneFolderFrontBrush" Color="{StaticResource DropZoneFolderFrontColor}" />
    <SolidColorBrush x:Key="DropZoneArrowBrush" Color="{StaticResource DropZoneArrowColor}" />
    <SolidColorBrush x:Key="DropZoneKbdBrush" Color="{StaticResource DropZoneKbdColor}" />
    <SolidColorBrush x:Key="SegmentSelectedHoverOverlayBrush" Color="{StaticResource SegmentSelectedHoverOverlayColor}" />
  </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>

            <!-- Compact mode uses the Fluent CheckBox template with a scale transform. -->
            <Geometry x:Key="CheckBoxIndeterminateMarkGeometry">M3,6 L9,6</Geometry>
        </ResourceDictionary>
    </Styles.Resources>

    <Style Selector="Window">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="Menu">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Padding" Value="0" />
    </Style>

    <Style Selector="TextBox">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <BrushTransition Property="Background" Duration="0:0:0.15" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="TextBox:focus">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <Style Selector="TextBox:pointerover">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <Style Selector="ComboBox">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <BrushTransition Property="Background" Duration="0:0:0.15" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="ComboBox:pointerover">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <Style Selector="ComboBox:focus">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <Style Selector="TreeView">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="TreeViewItem">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Padding" Value="4,1" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <!-- Compact mode - reduce tree row height without touching selection/hover behavior. -->
    <Style Selector="Window.compact-mode TreeViewItem">
        <Setter Property="Padding" Value="0,0" />
        <Setter Property="MinHeight" Value="0" />
    </Style>

    <!-- Reduce internal padding in TreeViewItem template for compact mode -->
    <Style Selector="Window.compact-mode TreeViewItem /template/ Border#PART_LayoutRoot">
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="0,-1" />
    </Style>

    <Style Selector="Window.compact-mode TreeViewItem /template/ ContentPresenter#PART_HeaderPresenter">
        <Setter Property="Margin" Value="0,-1" />
    </Style>

    <!-- Add horizontal gap between icon and text in compact mode -->
    <Style Selector="Window.compact-mode TreeView Image">
        <Setter Property="Margin" Value="0,0,4,0" />
    </Style>

    <Style Selector="Window.compact-mode TextBlock.section-title">
        <Setter Property="Margin" Value="0,8,0,2" />
        <Setter Property="FontSize" Value="11" />
    </Style>

    <Style Selector="Window.compact-mode Border.panel">
        <Setter Property="Padding" Value="8" />
    </Style>

    <Style Selector="Window.compact-mode Border.checklist-border">
        <Setter Property="Padding" Value="4" />
    </Style>

    <!-- Tree checkboxes: adjust these sizes to scale the checkbox square in compact mode. -->
    <Style Selector="Window.compact-mode TreeView CheckBox /template/ Border#PART_Box">
        <Setter Property="Width" Value="12" />
        <Setter Property="Height" Value="12" />
    </Style>

    <Style Selector="Window.compact-mode TreeView CheckBox /template/ Border#border">
        <Setter Property="Width" Value="12" />
        <Setter Property="Height" Value="12" />
    </Style>

    <Style Selector="Window.compact-mode TreeView CheckBox /template/ Border#PART_Bullet">
        <Setter Property="Width" Value="12" />
        <Setter Property="Height" Value="12" />
    </Style>

    <!-- Disable all hover and selection highlighting on TreeViewItem -->
    <Style Selector="TreeViewItem:pointerover">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="TreeViewItem:selected">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="TreeViewItem:selected:pointerover">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="TreeViewItem:selected:focus">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="TreeViewItem:pressed">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <!-- Override PART_LayoutRoot border inside TreeViewItem template (Fluent theme) -->
    <Style Selector="TreeViewItem /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:pointerover /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:selected /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:selected:pointerover /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:selected:focus /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:pressed /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:focus /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:selected:focus:pointerover /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!-- Also override ContentPresenter background in TreeViewItem header -->
    <Style Selector="TreeViewItem /template/ ContentPresenter#PART_HeaderPresenter">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:pointerover /template/ ContentPresenter#PART_HeaderPresenter">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:selected /template/ ContentPresenter#PART_HeaderPresenter">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style Selector="TreeViewItem:pressed /template/ ContentPresenter#PART_HeaderPresenter">
        <Setter Property="Background" Value="Transparent" />
    </Style>


    <Style Selector="Button">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Background" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.1" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Button:pointerover">
        <Setter Property="Background" Value="{DynamicResource AppBorderBrush}" />
    </Style>

    <Style Selector="Button:pressed">
        <Setter Property="RenderTransform" Value="scale(0.97)" />
    </Style>

    <Style Selector="Button.primary-action">
        <Setter Property="Background" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
        <Setter Property="RenderTransformOrigin" Value="50%,50%" />
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.22" Easing="CubicEaseInOut" />
                <BrushTransition Property="Background" Duration="0:0:0.18" Easing="CubicEaseInOut" />
                <BrushTransition Property="BorderBrush" Duration="0:0:0.18" Easing="CubicEaseInOut" />
            </Transitions>
        </Setter>
    </Style>

    <!-- Override Fluent template-level background for primary buttons -->
    <Style Selector="Button.primary-action /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.primary-action TextBlock">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.primary-action Path">
        <Setter Property="Fill" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.primary-action:pointerover">
        <Setter Property="Background" Value="{DynamicResource AppAccentHoverBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentHoverBrush}" />
        <Setter Property="RenderTransform" Value="scale(1.01)" />
    </Style>

    <Style Selector="Button.primary-action:pointerover /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AppAccentHoverBrush}" />
    </Style>

    <Style Selector="Button.primary-action:pressed">
        <Setter Property="Background" Value="{DynamicResource AppAccentPressedBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentPressedBrush}" />
        <Setter Property="Opacity" Value="0.95" />
        <Setter Property="RenderTransform" Value="scale(0.985)" />
    </Style>

    <Style Selector="Button.primary-action:pressed /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AppAccentPressedBrush}" />
    </Style>

    <Style Selector="ScrollViewer">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!-- ScrollBar thumb animations -->
    <Style Selector="ScrollBar /template/ Thumb">
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Background" Duration="0:0:0.12" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.1" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="ScrollBar /template/ Thumb:pointerover">
        <Setter Property="Background" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <!-- ToolTip animations -->
    <Style Selector="ToolTip">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
        <Setter Property="Opacity" Value="0.98" />
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.15" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="TextBlock.section-title">
        <Setter Property="Foreground" Value="{DynamicResource AppMutedTextBrush}" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="Margin" Value="0,14,0,6" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
    </Style>

    <Style Selector="TextBlock">
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <Style Selector="Border.panel">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="Padding" Value="10" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Background" Duration="0:0:0.2" Easing="CubicEaseOut" />
                <BrushTransition Property="BorderBrush" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Border.search-panel">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="10" />
        <Setter Property="ClipToBounds" Value="True" />
        <Setter Property="Margin" Value="0,2,0,0" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <DoubleTransition Property="Opacity" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>


    <Style Selector="Border.filter-panel">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="10" />
        <Setter Property="ClipToBounds" Value="True" />
        <Setter Property="Margin" Value="0,2,0,0" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <DoubleTransition Property="Opacity" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Border.preview-panel">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="10" />
        <Setter Property="ClipToBounds" Value="True" />
        <Setter Property="Margin" Value="0,2,0,0" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <DoubleTransition Property="Opacity" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Border.preview-line-numbers">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="0,0,1,0" />
        <Setter Property="Padding" Value="10,10,8,10" />
        <Setter Property="VerticalAlignment" Value="Top" />
    </Style>


    <!-- Rounded controls inside search/filter panels -->
    <Style Selector="Border.search-panel TextBox, Border.filter-panel TextBox">
        <Setter Property="CornerRadius" Value="6" />
    </Style>

    <Style Selector="Border.search-panel Button, Border.filter-panel Button">
        <Setter Property="CornerRadius" Value="6" />
    </Style>

    <Style Selector="Button.menu-icon-button">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="8,4" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="RenderTransformOrigin" Value="50%,50%" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Background" Duration="0:0:0.12" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.1" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Button.menu-icon-button:pointerover">
        <Setter Property="Background" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="RenderTransform" Value="scale(1.05)" />
    </Style>

    <Style Selector="Button.menu-icon-button:pressed">
        <Setter Property="RenderTransform" Value="scale(0.95)" />
    </Style>

    <Style Selector="StackPanel.settings-section">
        <Setter Property="Margin" Value="0,6,0,0" />
    </Style>

    <Style Selector="Border.checklist-border">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="10" />
        <Setter Property="ClipToBounds" Value="True" />
        <Setter Property="Padding" Value="5" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Background" Duration="0:0:0.2" Easing="CubicEaseOut" />
                <BrushTransition Property="BorderBrush" Duration="0:0:0.15" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Border.checklist-border:pointerover">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <!-- CheckBox base transitions -->
    <Style Selector="CheckBox">
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.1" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="CheckBox /template/ Border#NormalRectangle">
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="BorderBrush" Duration="0:0:0.12" Easing="CubicEaseOut" />
                <BrushTransition Property="Background" Duration="0:0:0.12" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="CheckBox:pointerover /template/ Border#NormalRectangle">
        <Setter Property="BorderBrush" Value="{DynamicResource AppAccentBrush}" />
    </Style>

    <Style Selector="CheckBox.list-item">
        <Setter Property="Margin" Value="0,1" />
    </Style>

    <Style Selector="Border.ignore-option-item">
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="0" />
    </Style>

    <Style Selector="Border.ignore-option-separator">
        <Setter Property="Height" Value="1" />
        <Setter Property="Background" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Margin" Value="0,0,0,2" />
    </Style>

    <Style Selector="CheckBox.section-all">
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="Margin" Value="0,6,4,-6" />
    </Style>

    <!-- Compact overrides after base checkbox styles to ensure precedence. -->
    <!-- We scale the control itself to keep Fluent visuals, rather than redefining the template. -->
    <!-- Settings list checkboxes: scale control to keep Fluent visuals. -->
    <!-- To make the checkbox larger/smaller, tweak ScaleX/ScaleY below. -->
    <!-- To change label size in compact mode, set FontSize here. -->
    <Style Selector="Window.compact-mode CheckBox.list-item">
        <Setter Property="Margin" Value="0" />
        <Setter Property="Padding" Value="0" />
        <!-- Increase MinHeight if you bump FontSize to avoid clipping. -->
        <Setter Property="MinHeight" Value="0" />
        <!-- Compact label font size for settings list items. -->
        <Setter Property="FontSize" Value="15" />
        <Setter Property="RenderTransformOrigin" Value="0,0.5" />
        <Setter Property="RenderTransform">
            <!-- Compact checkbox scale for settings list items. -->
            <ScaleTransform ScaleX="0.88" ScaleY="0.88" />
        </Setter>
    </Style>

    <!-- Ensure list-item labels track the CheckBox FontSize in compact mode. -->
    <Style Selector="Window.compact-mode CheckBox.list-item TextBlock">
        <Setter Property="FontSize" Value="{Binding $parent[CheckBox].FontSize}" />
    </Style>

    <!-- Keep padding between checkbox and label consistent in compact mode. -->
    <Style Selector="Window.compact-mode CheckBox.list-item /template/ ContentPresenter">
        <Setter Property="Margin" Value="6,0,0,0" />
        <!-- Fluent template can override FontSize on the presenter; force it here. -->
        <Setter Property="FontSize" Value="{Binding $parent[CheckBox].FontSize}" />
    </Style>

    <Style Selector="Window.compact-mode Border.ignore-option-separator">
        <Setter Property="Margin" Value="0,-4,0,3" />
    </Style>

    <!-- "All" checkboxes: same scale as list items, but text is compensated below. -->
    <Style Selector="Window.compact-mode CheckBox.section-all">
        <Setter Property="Margin" Value="0,4,2,-9" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="ClipToBounds" Value="False" />
        <Setter Property="RenderTransformOrigin" Value="0,0.5" />
        <Setter Property="RenderTransform">
            <!-- Keep checkbox visuals consistent with list items. -->
            <ScaleTransform ScaleX="0.88" ScaleY="0.88" />
        </Setter>
    </Style>

    <!-- Compensate text size for "All" checkboxes so label stays readable. -->
    <!-- If you change the checkbox scale, adjust this inverse scale accordingly. -->
    <!-- Increase these scale values to make "All" label font larger in compact mode. -->
    <Style Selector="Window.compact-mode CheckBox.section-all /template/ ContentPresenter">
        <Setter Property="Margin" Value="6,0,0,0" />
        <Setter Property="ClipToBounds" Value="False" />
        <Setter Property="RenderTransformOrigin" Value="0,0.5" />
        <Setter Property="RenderTransform">
            <ScaleTransform ScaleX="1.136" ScaleY="1.136" />
        </Setter>
    </Style>

    <Style Selector="ScrollViewer.checklist-scroll">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!-- Fix font inheritance for TextBlock in TreeView -->
    <Style Selector="TreeView TextBlock">
        <Setter Property="FontFamily" Value="{Binding $parent[TreeView].FontFamily}" />
        <Setter Property="FontSize" Value="{Binding $parent[TreeView].FontSize}" />
    </Style>

    <!-- Theme Popover Styles (Win11 Settings-like card) -->
    <Style Selector="Border.theme-popover">
        <Setter Property="Background" Value="{DynamicResource AppPanelBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="BoxShadow" Value="0 4 16 0 #40000000" />
        <Setter Property="RenderTransformOrigin" Value="50%,0%" />
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.15" Easing="CubicEaseOut" />
                <BrushTransition Property="Background" Duration="0:0:0.2" Easing="CubicEaseOut" />
                <BrushTransition Property="BorderBrush" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="TextBlock.popover-section-title">
        <Setter Property="Foreground" Value="{DynamicResource AppMutedTextBrush}" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="Margin" Value="0,0,0,2" />
    </Style>

    <!-- Popup Separator -->
    <Style Selector="Popup Separator">
        <Setter Property="Background" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Margin" Value="0,4" />
        <Setter Property="Height" Value="1" />
    </Style>

    <!-- Compact Slider in popover -->
    <Style Selector="Border.theme-popover Slider">
        <Setter Property="Margin" Value="0,2" />
        <Setter Property="MinHeight" Value="16" />
    </Style>

    <!-- ProgressBar styling -->
    <Style Selector="ProgressBar">
        <Setter Property="Foreground" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="Background" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Foreground" Duration="0:0:0.2" Easing="CubicEaseOut" />
                <BrushTransition Property="Background" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <!-- Slider styling -->
    <Style Selector="Slider">
        <Setter Property="Foreground" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Foreground" Duration="0:0:0.2" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Slider /template/ Thumb">
        <Setter Property="Transitions">
            <Transitions>
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.1" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Slider /template/ Thumb:pointerover">
        <Setter Property="RenderTransform" Value="scale(1.1)" />
    </Style>

    <!-- Menu and MenuItem styling -->
    <Style Selector="MenuItem">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
        <Setter Property="CornerRadius" Value="6" />
        <Setter Property="Margin" Value="2" />
    </Style>

    <Style Selector="MenuItem /template/ Border#PART_LayoutRoot">
        <Setter Property="CornerRadius" Value="6" />
    </Style>

    <Style Selector="MenuItem:pointerover /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="{DynamicResource MenuHoverBrush}" />
    </Style>

    <Style Selector="MenuItem:pressed /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="{DynamicResource MenuPressedBrush}" />
    </Style>

    <!-- Hard-disable TreeViewItem hover/selection background in Fluent template -->
    <Style Selector="TreeViewItem:pointerover /template/ Border">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
    </Style>


    <Style Selector="TreeViewItem:selected /template/ Border">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
    </Style>


    <Style Selector="TreeViewItem:pressed /template/ Border">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
    </Style>


    <Style Selector="TreeViewItem:selected:pointerover /template/ Border">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
    </Style>

    <!-- ContextMenu styling -->
    <Style Selector="ContextMenu">
        <Setter Property="Background" Value="{DynamicResource MenuPopupBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="RenderTransformOrigin" Value="50%,0%" />
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.12" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.12" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="MenuFlyoutPresenter">
        <Setter Property="Background" Value="{DynamicResource MenuPopupBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="RenderTransformOrigin" Value="50%,0%" />
        <Setter Property="Transitions">
            <Transitions>
                <DoubleTransition Property="Opacity" Duration="0:0:0.12" Easing="CubicEaseOut" />
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.12" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <!-- Menu popup backgrounds -->
    <Style Selector="MenuItem /template/ Popup > Border">
        <Setter Property="Background" Value="{DynamicResource MenuPopupBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="BoxShadow" Value="0 8 24 0 #2A000000" />
        <Setter Property="Padding" Value="4" />
    </Style>


    <!-- Submenu popup backgrounds -->
    <Style Selector="MenuItem MenuItem /template/ Popup > Border">
        <Setter Property="Background" Value="{DynamicResource MenuChildPopupBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="BoxShadow" Value="0 8 24 0 #2A000000" />
        <Setter Property="Padding" Value="4" />
    </Style>

    <!-- Submenu item backgrounds -->
    <Style Selector="MenuItem MenuItem">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="CornerRadius" Value="5" />
        <Setter Property="Margin" Value="2,1" />
    </Style>

    <Style Selector="MenuItem MenuItem /template/ Border#PART_LayoutRoot">
        <Setter Property="CornerRadius" Value="5" />
    </Style>

    <Style Selector="MenuItem MenuItem:pointerover /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="{DynamicResource MenuChildHoverBrush}" />
    </Style>

    <Style Selector="MenuItem MenuItem:pressed /template/ Border#PART_LayoutRoot">
        <Setter Property="Background" Value="{DynamicResource MenuChildPressedBrush}" />
    </Style>

    <!-- ============================================== -->
    <!-- Segmented Control Styles                       -->
    <!-- ============================================== -->

    <!-- Container: pill-shaped border around the entire segmented control -->
    <Style Selector="Border.segmented-control">
        <Setter Property="Background" Value="{DynamicResource AppBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{DynamicResource AppBorderBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="CornerRadius" Value="5" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <!-- Sliding thumb indicator behind the selected segment -->
    <Style Selector="Border.segment-thumb">
        <Setter Property="Background" Value="{DynamicResource AppAccentBrush}" />
        <Setter Property="CornerRadius" Value="3" />
        <Setter Property="RenderTransform" Value="translate(0px, 0px)" />
        <Setter Property="Transitions">
            <Transitions>
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.2" Easing="CubicEaseInOut" />
            </Transitions>
        </Setter>
    </Style>

    <!-- Thumb moved to right position (JSON selected) -->
    <Style Selector="Border.segment-thumb.segment-thumb-right">
        <Setter Property="RenderTransform" Value="translate(48px, 0px)" />
    </Style>

    <!-- Text button for segment (transparent, overlays the thumb) -->
    <Style Selector="Button.segment-text-button">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Foreground" Value="{DynamicResource AppMutedTextBrush}" />
        <Setter Property="Padding" Value="0,3" />
        <Setter Property="Width" Value="48" />
        <Setter Property="MinHeight" Value="0" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="Transitions">
            <Transitions>
                <BrushTransition Property="Foreground" Duration="0:0:0.15" Easing="CubicEaseOut" />
            </Transitions>
        </Setter>
    </Style>

    <!-- Segment text button: hover (unselected) -->
    <Style Selector="Button.segment-text-button:pointerover">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{DynamicResource AppTextBrush}" />
    </Style>

    <!-- Segment text button: pressed -->
    <Style Selector="Button.segment-text-button:pressed">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Opacity" Value="0.8" />
    </Style>

    <!-- Segment text button: selected state -->
    <Style Selector="Button.segment-text-button.segment-selected">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.segment-text-button.segment-selected /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.segment-text-button.segment-selected TextBlock">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <!-- Segment text button: selected + hover -->
    <Style Selector="Button.segment-text-button.segment-selected:pointerover">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
        <Setter Property="Background" Value="{DynamicResource SegmentSelectedHoverOverlayBrush}" />
    </Style>

    <Style Selector="Button.segment-text-button.segment-selected:pointerover /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource SegmentSelectedHoverOverlayBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <Style Selector="Button.segment-text-button.segment-selected:pointerover TextBlock">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryActionForegroundBrush}" />
    </Style>

    <!-- Segment control: disabled -->
    <Style Selector="Border.segmented-control:disabled">
        <Setter Property="Opacity" Value="0.5" />
    </Style>

    <Style Selector="Border.preview-segmented-control">
        <Setter Property="Padding" Value="2" />
        <Setter Property="Width" Value="544" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="ClipToBounds" Value="True" />
    </Style>

    <Style Selector="Border.preview-segment-thumb">
        <Setter Property="Width" Value="180" />
        <Setter Property="Transitions">
            <Transitions>
                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.25" Easing="CubicEaseInOut" />
            </Transitions>
        </Setter>
    </Style>

    <Style Selector="Border.preview-segment-thumb.preview-segment-thumb-middle">
        <Setter Property="RenderTransform" Value="translate(180px, 0px)" />
    </Style>

    <Style Selector="Border.preview-segment-thumb.preview-segment-thumb-right">
        <Setter Property="RenderTransform" Value="translate(360px, 0px)" />
    </Style>

    <Style Selector="Button.preview-segment-button">
        <Setter Property="Width" Value="180" />
        <Setter Property="Padding" Value="0,5" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>

</Styles>
